{
  "name": "nativescript-xmpp-client",
  "version": "1.1.0",
  "description": "XMPP websocket client plugin for Nativescript.",
  "main": "xmpp-client.js",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.3.0",
      "ios": "2.3.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
    "demo.ios.device": "npm run preparedemo && cd demo && tns run ios",
    "demo.android": "npm run preparedemo && cd demo && tns run android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-xmpp-client && tns plugin add .. && tns install",
    "setup": "npm i && cd demo && npm i && cd .. && npm run build && cd demo && tns plugin add .. && cd ..",
    "postclone": "npm i && node scripts/postclone.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelagm/nativescript-xmpp-client.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "XMPP",
    "WebSocket",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Samuel Agbonkpolo",
    "email": "samuelagm@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/samuelagm/nativescript-xmpp-client/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/samuelagm/nativescript-xmpp-client",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^2.3.0",
    "tns-platform-declarations": "^2.3.0",
    "typescript": "^1.8.10",
    "prompt": "^1.0.0",
    "rimraf": "^2.5.0"
  },
  "dependencies": {
    "@types/node": "^10.12.21",
    "@xmpp/jid": "^0.0.1",
    "@xmpp/xml": "^0.0.1",
    "base64-js": "1.2.0",
    "debug": "^2.2.0",
    "inherits": "^2.0.3",
    "ltx-ea": "^2.5.1",
    "md5.js": "^1.3.3",
    "nativescript-websockets": "^1.3.0",
    "net-browserify": "^0.2.4",
    "util": "^0.10.3"
  },
  "_id": "nativescript-xmpp-client@1.0.0",
  "_shasum": "408b8726d830a299ead191d8c89777d71036f619",
  "_from": "..",
  "_resolved": "file:.."
}
